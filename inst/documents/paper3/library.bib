@article{Eckhoff2015,
abstract = {Since the original Ross-Macdonald formulations of vector-borne disease transmission, there has been a broad proliferation of mathematical models of vector-borne disease, but many of these models retain most to all of the simplifying assumptions of the original formulations. Recently, there has been a new expansion of mathematical frameworks that contain explicit representations of the vector life cycle including aquatic stages, multiple vector species, host heterogeneity in biting rate, realistic vector feeding behavior, and spatial heterogeneity. In particular, there are now multiple frameworks for spatially explicit dynamics with movements of vector, host, or both. These frameworks are flexible and powerful, but require additional data to take advantage of these features. For a given question posed, utilizing a range of models with varying complexity and assumptions can provide a deeper understanding of the answers derived from models.},
author = {Eckhoff, Philip A. and Bever, Caitlin A. and Gerardin, Jaline and Wenger, Edward A. and Smith, David L.},
doi = {10.1016/j.cois.2015.05.002},
file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/eckhoff2015{\_}puddles{\_}to{\_}planet{\_}malaria{\_}modelling{\_}review.pdf:pdf},
issn = {22145745},
journal = {Current Opinion in Insect Science},
pages = {118--123},
publisher = {Elsevier Inc},
title = {{From puddles to planet: Modeling approaches to vector-borne diseases at varying resolution and scale}},
url = {http://dx.doi.org/10.1016/j.cois.2015.05.002},
volume = {10},
year = {2015}
}
@article{Fouet2018,
abstract = {One of the most common strategies for controlling mosquito-borne diseases relies on the use of chemical pesticides to repel or kill the mosquito vector. Pesticide exposure interferes with several key biological functions in the mosquito and triggers a variety of adaptive responses whose underlying mechanisms are only partially elucidated. The availability of reference genome sequences opens up the possibility of tracking signatures of evolutionary changes, including the most recent, across the genomes of many vector species. In this review, we highlight the recent genomic changes, which contribute to the fascinating adaptation of malaria vectors of the sub-Saharan African region to intensive insecticide-based interventions. We emphasize the operational significance of detailed genomic knowledge for current monitoring and decision-making. Widespread use of insecticides provides an opportunity to examine the adaptive responses of target species to human interventions.Rapid evolution of anopheline mosquitoes represents a potential threat to any vector-based malaria control effort.The use of high-quality genomic data and gene-editing tools provides powerful strategies for characterizing insecticide resistance genes and their functions in mosquitoes.A better understanding of the genetic, behavioral and physiological mechanisms underlying insecticide resistance will yield potentially vital knowledge for vector-borne disease control.},
author = {Fouet, Caroline and Atkinson, Peter and Kamdem, Colince},
doi = {10.1016/j.pt.2017.10.012},
file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/fouet2018{\_}human-interventions-driving-mosquito-evolution-genomics.pdf:pdf},
isbn = {1471-4922},
issn = {14715007},
journal = {Trends in Parasitology},
keywords = {Adaptation,Anopheles,Genome,Insecticides,Vector control},
pages = {1--13},
pmid = {29301722},
publisher = {Elsevier Ltd},
title = {{Human Interventions: Driving Forces of Mosquito Evolution}},
url = {http://dx.doi.org/10.1016/j.pt.2017.10.012},
volume = {xx},
year = {2018}
}
@article{Day2015,
abstract = {The emergence of drug-resistant pathogens is often considered a canonical case of evolution by natural selection. Here we argue that the strength of selection can be a poor predictor of the rate of resistance emergence. It is possible for a resistant strain to be under negative selection and still emerge in an infection or spread in a population. Measuring the right parameters is a necessary first step toward the development of evidence-based resistance-management strategies. We argue that it is the absolute fitness of the resistant strains that matters most and that a primary determinant of the absolute fitness of a resistant strain is the ecological context in which it finds itself.},
archivePrefix = {arXiv},
arxivId = {15334406},
author = {Day, Troy and Huijben, Silvie and Read, Andrew F.},
