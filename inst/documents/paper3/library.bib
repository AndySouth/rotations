@article{Casida2009,
abstract = {This chapter highlights the diversity of mechanisms for both pesticidal activity and selectivity. Pesticides are intended to disrupt a primary target in the pest so it is no longer harmful. The pesticide per se or as its bioactivated form binds to or interacts with a specific enzyme, receptor, protein, or membrane, initiating a series of events that is deleterious or lethal to the pest. The primary interaction usually occurs with the pesticide at picomolar or nanomolar levels and secondary interactions at higher concentrations. However, this is not always the case. There may be several targets of similar sensitivity but not equal importance. The discovery of each new type of pesticide is followed by structural optimization for the highest possible potency involving sequential modification of each substituent to best fit the target site. Most insecticides by number, amount, and market value act on the nervous system at the synapse or the axon. The cholinergic system is the principal insecticide target with OP and MC compounds inhibiting AChE to prolong the excitatory action of acetylcholine. Pesticide management is a major aspect of pest control. However, we can still achieve higher target site potency and lower use levels, reduced environmental impact, and improved safety. Pesticide legislation policies for risks and hazards continue a trend toward ever-greater restrictions. ?? 2010 Elsevier Inc. All rights reserved.},
author = {Casida, John E.},
doi = {10.1016/B978-0-12-374367-1.00002-1},
file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/casida2009{\_}mechanisms-of-pesticide-action.pdf:pdf},
isbn = {9780123743671},
issn = {0893228X},
journal = {Chemical Research in Toxicology},
pages = {609--619},
pmid = {19284791},
title = {{Pest toxicology: The primary mechanisms of pesticide action}},
volume = {22},
year = {2009}
}
@article{Zhao2003,
abstract = {Preventing insect pests from developing resistance to Bacillus thuringiensis (Bt) toxins produced by transgenic crops is a major challenge for agriculture. Theoretical models suggest that plants containing two dissimilar Bt toxin genes ('pyramided' plants) have the potential to delay resistance more effectively than single-toxin plants used sequentially or in mosaics. To test these predictions, we developed a unique model system consisting of Bt transgenic broccoli plants and the diamondback moth, Plutella xylostella. We conducted a greenhouse study using an artificial population of diamondback moths carrying genes for resistance to the Bt toxins Cry1Ac and Cry1C at frequencies of about 0.10 and 0.20, respectively. After 24 generations of selection, resistance to pyramided two-gene plants was significantly delayed as compared with resistance to single-gene plants deployed in mosaics, and to Cry1Ac toxin when it was the first used in a sequence. These results have important implications for the development and regulation of transgenic insecticidal plants.},
author = {Zhao, Jian-Zhou and Cao, Jun and Li, Yaxin and Collins, Hilda L and Roush, Richard T and Earle, Elizabeth D and Shelton, Anthony M},
doi = {10.1038/nbt907},
file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/Zhao2003{\_}2{\_}Bt{\_}toxins{\_}delay{\_}resistance.pdf:pdf},
isbn = {1087-0156},
issn = {1087-0156},
journal = {Nature Biotechnology},
number = {12},
pages = {1493--1497},
pmid = {14608363},
title = {{Transgenic plants expressing two Bacillus thuringiensis toxins delay insect resistance evolution}},
volume = {21},
year = {2003}
}
