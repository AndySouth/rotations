file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/kolaczinski2000{\_}pyrethroid-mortality-resistant-susceptible-and-F1.pdf:pdf},
isbn = {0007-4853 (Print)},
