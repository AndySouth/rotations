}
@article{Djenontin2015,
abstract = {In the context of the widespread distribution of pyrethroid resistance among malaria vectors, we did a release-recapture trial in experimental huts to investigate the insecticidal and sterilizing effects of a novel long-lasting net (LN), Olyset(R) Duo, incorporating a mixture of permethrin (PER) and the insect growth regulator (IGR), pyri-proxyfen (PPF). An LN containing PPF alone and a classic Olyset(R) Net were tested in parallel as positive controls. The effect of progressive number of holes (6, 30, or 150) that may accrue in nets over time was simulated. We used two laboratory Anopheles gambiae s.s. strains: the susceptible Kisumu strain and the pyrethroid-resistant VK-Per strain having solely kdr as resistance mechanism. The effect of these nets on the reproductive success of blood-fed females that survived the different LNs conditions was recorded. Regardless of the mosquito strain, the LNs containing PPF alone with as many as 30 holes drastically reduced the number of eggs laid by females succeeding in feeding, i.e. fecundity by 98{\%} and egg hatching rate (fertility) by 93{\%} relative to untreated control net. Very few of the resistant females blood fed and survived under the Olyset(R) Duo with similar number of holes (up to 30) but of these few, the inhibition of reproductive success was 100{\%}. There was no evidence that the Olyset(R) Duo LN with 150 holes impacted fecundity or fertility of the resistant colony. The efficacy of Olyset(R) Duo is encouraging and clearly illustrates that this new net might be a promising tool for malaria transmission control and resistance management.},
author = {Dj{\`{e}}nontin, Armel and {Ahoua Alou}, Ludovic P. and Koffi, Alphonsine and Zogo, Barnabas and Duarte, Elves and N'Guessan, Raphael and Moiroux, Nicolas and Pennetier, C{\'{e}}dric},
doi = {10.1051/parasite/2015027},
file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/Djenontin2015{\_}Olyset-Duo-nets-pyr-susceptible-resistant-an-gamb.pdf:pdf},
issn = {1776-1042},
journal = {Parasite},
keywords = {anopheles gambiae s,anopheles gambiae sensibles et,aux,bed net,d,dans le contexte d,effets insecticide et st{\'{e}}rilisant,expansion de la r{\'{e}}sistance,insect growth regulator,l{\^{a}}cher-recapture en cases exp{\'{e}}rimentales,moustiquaire impr{\'{e}}gn{\'{e}}e avec un,m{\'{e}}lange de,no{\"{i}}des,olyset {\`{o}} duo,perm{\'{e}}thrine et de pyriproxyfen,pyrethroid,resistance,r{\'{e}}sistantes aux pyr{\'{e}}thri-,r{\'{e}}sum{\'{e}},s,sur des souches d},
