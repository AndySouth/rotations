journal = {Malaria Journal},
keywords = {Acetylcholinesterase,Acetylcholinesterase: genetics,Animals,Anopheles gambiae,Anopheles gambiae: classification,Anopheles gambiae: drug effects,Anopheles gambiae: genetics,Bedding and Linens,Chlorpyrifos,Chlorpyrifos: adverse effects,Chlorpyrifos: analogs {\&} derivatives,Chlorpyrifos: pharmacology,Culex,Culex: classification,Culex: drug effects,Culex: genetics,Drug Combinations,Female,Genotype,Humans,Insect Vectors,Insect Vectors: classification,Insect Vectors: drug effects,Insect Vectors: genetics,Insecticide Resistance,Insecticide Resistance: genetics,Insecticides,Insecticides: pharmacology,Malaria,Malaria: prevention {\&} control,Mosquito Control,Mosquito Control: methods,Nitriles,Nitriles: adverse effects,Nitriles: pharmacology,Pyrethrins,Pyrethrins: adverse effects,Pyrethrins: pharmacology,Treatment Outcome},
