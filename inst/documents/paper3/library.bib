isbn = {1526-4998},
issn = {1526498X},
journal = {Pest Management Science},
keywords = {Fitness cost,Insecticide resistance,Pest,Selection},
number = {11},
pages = {1431--1437},
pmid = {22945853},
title = {{Fitness costs associated with insecticide resistance}},
volume = {68},
