issn = {0007-4853},
journal = {Bulletin of entomological research},
keywords = {Adult,Alleles,Animals,Anopheles,Anopheles: genetics,Bedding and Linens,Carbamates,Culex,Culex: genetics,Feeding Behavior,Humans,Insect Repellents,Insecticide Resistance,Insecticide Resistance: genetics,Insecticides,Male,Mosquito Control,Mosquito Control: methods,Organothiophosphorus Compounds,Pesticide Residues,Pyrethrins,Sodium Channels,Sodium Channels: genetics},
number = {2},
