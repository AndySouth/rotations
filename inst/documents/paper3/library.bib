}
@article{Donnelly2009,
abstract = {Several groups are developing and applying DNA-based technologies to monitor insecticide-based disease control programmes. However, several recent papers have concluded that the knockdown resistance (kdr) genotype-phenotype correlation that is observed in a wide variety of taxa might not hold in all mosquitoes. In this article, we review the evidence to support this putative breakdown and argue that the conclusion follows from unreliable data or the unparsimonious interpretation of data. We assert that the link between kdr genotype and DDT- and pyrethroid-susceptibility phenotype is clear. However, we emphasize that kdr genotype might explain only a portion of heritable variation in resistance and that diagnostic assays to test the importance of other resistance mechanisms in field populations are required. {\textcopyright} 2009 Elsevier Ltd. All rights reserved.},
author = {Donnelly, Martin J. and Corbel, Vincent and Weetman, David and Wilding, Craig S. and Williamson, Martin S. and {Black IV}, William C.},
doi = {10.1016/j.pt.2009.02.007},
file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/donnelly2009{\_}does-kdr-genotype-predict-insecticide-resistance-phenotype.pdf:pdf},
isbn = {1471-5007 (Electronic)$\backslash$r1471-4922 (Linking)},
issn = {14714922},
journal = {Trends in Parasitology},
number = {5},
pages = {213--219},
pmid = {19369117},
title = {{Does kdr genotype predict insecticide-resistance phenotype in mosquitoes?}},
volume = {25},
year = {2009}
}
@article{Gardner1999,
abstract = {The evolution of pesticide and drug resistance plagues agriculture and medicine worldwide. Resistance may stem from a single major gene or from quantitative sources, and both have been observed in the same species. The genetic basis of resistance plays a key role in treatment, as low concentrations facilitate the evolution of quantitative resistance, while high concentrations select for the rapid appearance of major gene resistance. When crop rotation or pesticide rotation is unfeasible (e.g. orchard crops or cross resistance to multiple pesticides), the authors propose and model a strategy of rotating low and high concentrations in alternate treatments. This strategy should delay resistance and extend pest control longer than using a series of constant low or high concentrations. The model framework was modified to investigate whether inducible (compared with constitutive) defence may also slow the evolution of pest resistance to plant secondary compounds, with possible implications for genetic engineering of pest resistant crops. The model led to predictions that plants with inducible defenses supported pests that evolved resistance more slowly than plants with constitutive defenses, suggesting that natural selection has favoured a strategy similar to pesticide concentration rotation.},
author = {Gardner, Shea N and Agrawal, A A and Gressel, Jonathan and Mangel, Marc},
file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/gardner1999{\_}strategies-to-delay-evolution-of-resistance-in-pests.pdf:pdf},
isbn = {0265-1491},
issn = {0265-1491},
journal = {Aspects of Applied Biology},
keywords = {Biotechnology (General) [WW000] Pathogen,Pa,Pest},
pages = {189--196},
title = {{Strategies to delay the evolution of resistance in pests: dose rotations and induced plant defenses}},
url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS{\&}CSC=Y{\&}NEWS=N{\&}PAGE=fulltext{\&}D=caba4{\&}AN=19991109224 http://library.wur.nl/sfx{\_}local?sid=OVID:cabadb{\&}id=pmid:{\&}id=doi:{\&}issn=0265-1491{\&}isbn={\&}volume={\&}issue=53{\&}spage=189{\&}pages=189-196{\&}date=1999{\&}title=Aspects+of+Applied+Bio},
volume = {53},
year = {1999}
}
@article{Gressel2011,
abstract = {At very low pesticide rates, a certain low proportion of pests may receive a sublethal dose, are highly stressed by the pesticide and yet survive. Stress is a general enhancer of mutation rates. Thus, the survivors are likely to have more than normal mutations, which might include mutations leading to pesticide resistance, both for multifactorial (polygenic, gene amplification, sequential allelic mutations) and for major gene resistance. Management strategies should consider how to eliminate the subpopulation of pests with the high mutation rates, but the best strategy is probably to avoid too low application rates of pesticides from the outset.},
author = {Gressel, Jonathan},
doi = {10.1002/ps.2071},
file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/gressel2010{\_}low-pesticide-may-hasten-resistance-by-mutation.pdf:pdf},
isbn = {1526-4998},
issn = {1526498X},
journal = {Pest Management Science},
keywords = {Low rates,Mutation frequency,Mutator genes,Resistance,Stress-induced mutations},
number = {3},
pages = {253--257},
pmid = {21308950},
title = {{Low pesticide rates may hasten the evolution of resistance by increasing mutation frequencies}},
volume = {67},
year = {2011}
}
