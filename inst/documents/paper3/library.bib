issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Adaptation,Amino Acid Substitution,Amino Acid Substitution: genetics,Animals,Australasia,Diptera,Diptera: genetics,Evolution,Genes,Genetic,Genetic: genetics,Haplotypes,Insect,Insect: genetics,Insecticide Resistance,Insecticide Resistance: genetics,Molecular,Molecular Sequence Data,Organophosphorus Compounds,Phylogeny,Physiological,Physiological: genetics,Polymerase Chain Reaction,Polymorphism,Tissue Preservation},
