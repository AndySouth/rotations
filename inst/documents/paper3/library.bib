issn = {14715007},
journal = {Trends in Parasitology},
keywords = {climate change,epidemiology,hazard,model,risk,vector-borne diseases},
number = {3},
pages = {227--245},
pmid = {29229233},
publisher = {Elsevier Ltd},
title = {{Mosquito-Borne Diseases: Advances in Modelling Climate-Change Impacts}},
url = {http://dx.doi.org/10.1016/j.pt.2017.11.006},
volume = {34},
year = {2018}
}
