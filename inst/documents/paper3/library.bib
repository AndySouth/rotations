isbn = {0022-0493 (Print)$\backslash$r0022-0493},
issn = {0022-0493},
journal = {Journal of economic entomology},
keywords = {Fitness costs,Insecticide resistance,[Economics,among arthropod,clearest,economics,evolution of insecticide resistance,examples of the economic,insecticide resistance,malaria,pests and disease vectors,provides one of the,relevance of evolutionary,vector control,þtness costs},
number = {May},
