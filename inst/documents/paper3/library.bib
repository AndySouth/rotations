title = {{Acetylcholinesterase (Ace-1) target site mutation 119S is strongly diagnostic of carbamate and organophosphate resistance in {\textless}i{\textgreater}Anopheles gambiae s.s.{\textless}/i{\textgreater} and {\textless}i{\textgreater}Anopheles coluzzii{\textless}/i{\textgreater} across southern Ghana}},
