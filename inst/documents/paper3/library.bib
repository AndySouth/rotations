@article{Brooke2010,
abstract = {Insecticide use in public health and agriculture presents a dramatic adaptive challenge to target and non-target insect populations. The rapid development of genetically modulated resistance to insecticides is postulated to develop in two distinct ways: By selection for single major effect genes or by selection for loose confederations in which several factors, not normally associated with each other, inadvertently combine their effects to produce resistance phenotypes. Insecticide resistance is a common occurrence and has been intensively studied in the major malaria vector Anopheles gambiae, providing a useful model for examining how insecticide resistance develops and what pleiotropic effects are likely to emerge as a consequence of resistance. As malaria vector control becomes increasingly reliant on successfully managing insecticide resistance, the characterisation of resistance mechanisms and their pleiotropic effects becomes increasingly important.},
author = {Brooke, Basil D. and Koekemoer, Lizette L.},
doi = {10.1186/1756-3305-3-74},
file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/brooke2010{\_}major-effect-genes-or-loose-confederations-insecticide-resistance-An-gambiae.pdf:pdf},
isbn = {1756-3305 (Electronic)$\backslash$n1756-3305 (Linking)},
issn = {17563305},
journal = {Parasites and Vectors},
number = {1},
pages = {1--8},
pmid = {20716346},
title = {{Major effect genes or loose confederations? the development of insecticide resistance in the malaria vector Anopheles gambiae}},
volume = {3},
year = {2010}
}
@article{Comins1977,
abstract = {Although the costs of pesticide resistance are hard to quantify, they are an important consideration in choosing control strategies for many pest situations. This paper derives a method for choosing optimal pesticide strategies which takes these costs into account. It is found in the case of a multivoltine pest (given certain general genetic assumptions) that the available strategies can be reduced to a one-parameter set. The final choice which requires the quantification of resistance cost can thus be made graphically. The effect of resistance cost is found to be equivalent to an increase in the cost of pesticide. Finally some general rules are derived for the allocation of pesticide susceptibility (which may be treated as a depletable resource) between seasons having different numbers of pests. The case of the sugarcane froghopper is treated as an example of the method. {\textcopyright} 1977.},
author = {Comins, Hugh N.},
doi = {10.1016/0022-5193(77)90206-5},
file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/comins1977.pdf:pdf},
isbn = {0022-5193},
issn = {10958541},
journal = {Journal of Theoretical Biology},
number = {3},
pages = {399--420},
pmid = {404477},
title = {{The management of pesticide resistance}},
volume = {65},
year = {1977}
}
