journal = {Bulletin of entomological research},
number = {2000},
pages = {3--7},
pmid = {10948358},
publisher = {Liverpool School Of Tropical Medicine},
title = {{The character or the variation: the genetic analysis of the insecticide-resistance phenotype.}},
volume = {90},
year = {2000}
}
