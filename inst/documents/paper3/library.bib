isbn = {1475-2875 (Electronic)$\backslash$r1475-2875 (Linking)},
issn = {1475-2875},
journal = {Malaria Journal},
keywords = {Insecticide resistance,Malaria vectors,GPIRM,Resou,gpirm,insecticide resistance,malaria vectors,resource deficiencies},
number = {1},
pages = {173},
pmid = {25899397},
publisher = {???},
title = {{Implementation of the global plan for insecticide resistance management in malaria vectors: progress, challenges and the way forward}},
